
<div>
<%= form_with model: forme do |form| %>
    <div>
      <%= form.label :name %><br>
      <%= form.text_field :name %>
     </div>

    <div>
      <%= form.label :description %><br>
      <%= form.text_area :description %>
     </div>
     
    <div>
    <%= form.label :cat %>
    <%= form.select :cat_id,options_for_select(@cat.collect {|cat| [cat.typ,cat.id]}) %>   
    </div>

     <div>
        <%= form.submit %>
     </div>

<% end %>
</div>

